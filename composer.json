{
    "name": "somardesignstudios/silverstripe-mapdrawingfield",
    "description": "Provides an interface for drawing geocoded shapes using Google Maps",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "maps",
        "googlemaps"
    ],
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=5.4.0",
        "silverstripe/vendor-plugin": "^1",
        "silverstripe/cms": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Somar\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    }
}
