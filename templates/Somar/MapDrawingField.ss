<div class="mapdrawingfield" $AttributesHTML>
    <div class="mapdrawingfield__holder" style="width:100%;padding-top:56.25%;">
        <p><%t Somar\MapDrawingField.NOMAP "If the map is not showing, please refresh the page" %></p>
    </div>
    <textarea id="$ID" name="$Name" style="display:none;">$Value.RAW</textarea>
    <div class="mapdrawingfield__controls" style="display:none;margin-top:.5rem;">
        <button type="button" class="btn btn-dark mapdrawingfield__clear"><%t Somar\MapDrawingField.CLEARMAP "Clear Map" %></button>
    </div>
</div>
